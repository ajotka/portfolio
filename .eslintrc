{
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 6,
        "requireConfigFile": false
    },
    "extends": "airbnb-base",
    "env": {
        "browser": true
    },
    "settings": {
        "import/resolve": {
            "moduleDirectory": [
                "node_modules",
                "src"
            ]
        }
    },
    "rules": {
        "no-console": "warn",
        "no-debugger": "warn",
        "indent": ["error", 4],
        "class-methods-use-this": "warn"
    }
}
